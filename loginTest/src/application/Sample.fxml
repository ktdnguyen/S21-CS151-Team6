<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="application.SampleController">
   <children>
      <Label alignment="TOP_CENTER" prefHeight="48.0" prefWidth="268.0" text="Wallet Manager">
         <font>
            <Font size="36.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <Label text="Sign In">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Label>
      <TextField prefHeight="25.0" prefWidth="425.0" promptText="User">
         <VBox.margin>
            <Insets left="150.0" right="150.0" />
         </VBox.margin>
      </TextField>
      <TextField promptText="Password">
         <VBox.margin>
            <Insets left="150.0" right="150.0" />
         </VBox.margin>
      </TextField>
      <Button mnemonicParsing="false" prefWidth="95.0" text="Submit">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button mnemonicParsing="false" text="Sign Up" />
   </children>
</VBox>
